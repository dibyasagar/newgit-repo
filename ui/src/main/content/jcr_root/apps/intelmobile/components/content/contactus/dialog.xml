<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Contact Us "
    width="100%"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Title Details">
                <items jcr:primaryType="cq:WidgetCollection">
                    <contactusheading
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Contact Us Heading"
                        name="./contactusheading"
                        xtype="textfield"/>
                    <namelabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Name Label"
                        name="./namelabel"
                        xtype="textfield"/>
                    <questionlabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Question label"
                        name="./questionlabel"
                        xtype="textfield"/>
                    <emaillabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Email label"
                        name="./emaillabel"
                        xtype="textfield"/>
                    <signmetext
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Sign me Text"
                        name="./signmetext"
                        xtype="textfield"/>
                    <dontsignmetext
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Sign me Text"
                        name="./dontsignmetext"
                        xtype="textfield"/>
                    <destinationmailaddr
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Destination Email Address"
                        name="./destinationmailaddr"
                        xtype="textfield"/>
                </items>
            </tab1>
            <pagelisttab
                jcr:primaryType="cq:Panel"
                title="Questions">
                <items jcr:primaryType="cq:WidgetCollection">
                    <hero
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./questions"
                        fieldDescription="Click '+' to add a new question"
                        fieldLabel="question"
                        maxItems="20"
                        name="./questions"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <title
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Question"
                                name="./question"
                                xtype="textfield"/>
                        </fieldConfig>
                    </hero>
                </items>
            </pagelisttab>
        </items>
    </items>
</jcr:root>
