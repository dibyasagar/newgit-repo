<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="Global Footer"
    width="100%"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Footer Navigation Links">
                <items jcr:primaryType="cq:WidgetCollection">
                    <multifrontfeeds
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./footerNavigationlinks"
                        fieldDescription="Click the '+' to add a new Link."
                        fieldLabel="Global Footer Links"
                        name="./footerNavigationlinks"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <title
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Global Footer Display Text"
                                name="./footerTitle"
                                xtype="textfield"/>
                            <url
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                editable="{Boolean}true"
                                fieldDescription="For External URL start the URL with 'http://'"
                                fieldLabel="Global Footer URL"
                                name="./footerUrl"
                                predicate="hierarchy"
                                xtype="pathfield"/>
                            <sameornewwindow
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}true"
                                defaultValue="no"
                                fieldLabel="Open in new window"
                                name="./newwindow"
                                type="select"
                                value="no"
                                width="20"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <no
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="no"/>
                                    <yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="yes"/>
                                </options>
                            </sameornewwindow>
                        </fieldConfig>
                    </multifrontfeeds>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="cq:Panel"
                title="Footer Details">
                <items jcr:primaryType="cq:WidgetCollection">
                    <fullsitelinktext
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        defaultValue="Visit Full Site"
                        fieldLabel="Full Site Link Text"
                        name="./fullsitelinktext"
                        xtype="textfield"/>
                    <fullsitelink
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldDescription="For External URL start the URL with 'http://'"
                        fieldLabel="Full Site Link"
                        name="./fullsitelink"
                        xtype="pathfield"/>
                    <sameornewwindow
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}true"
                        defaultValue="yes"
                        fieldLabel="Open full site in new window"
                        name="./fullsitenewwindow"
                        type="select"
                        width="20"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <no
                                jcr:primaryType="nt:unstructured"
                                text="No"
                                value="no"/>
                            <yes
                                jcr:primaryType="nt:unstructured"
                                text="Yes"
                                value="yes"/>
                        </options>
                    </sameornewwindow>
                    <hidefullsitelink
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Hide Fullsite View Link"
                        id="hidefullsite"
                        name="./hidefullsitelink"
                        type="checkbox"
                        xtype="selection"/>
                    <trademarktext
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        defaultValue="Â© Intel Corporation"
                        fieldLabel="Trademark Text"
                        name="./trademarktext"
                        xtype="textfield"/>
                    <region
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        defaultValue="US(English)"
                        fieldLabel="Enter Region"
                        name="./region"
                        xtype="textfield"/>
                    <regionurl
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Region Url"
                        name="./regionurl"
                        xtype="pathfield"/>
                    <hideregionurl
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Hide Region URL"
                        id="hideregionurl"
                        name="./hideregionurl"
                        type="checkbox"
                        xtype="selection"/>
                </items>
            </tab2>
        </items>
    </items>
</jcr:root>
