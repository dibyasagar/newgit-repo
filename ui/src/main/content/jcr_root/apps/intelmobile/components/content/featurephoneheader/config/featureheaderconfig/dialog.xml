<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Featurephone Header Component"
    width="100%"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <logoimage
                jcr:primaryType="cq:Widget"
                allowUpload="{Boolean}false"
                ddGroups="[media]"
                fileNameParameter="./logoImageName"
                fileReferenceParameter="./logoImageReference"
                id="logoimage"
                name="./logoimagename"
                title="Logo Image"
                xtype="smartimage"/>
            <tab2
                jcr:primaryType="cq:Panel"
                title="Logo Link">
                <items jcr:primaryType="cq:WidgetCollection">
                    <linkpath
                        jcr:primaryType="cq:Widget"
                        editable="{Boolean}true"
                        fieldDescription="Link Path of the Logo"
                        fieldLabel="Link Path"
                        name="./linkpath"
                        predicate="hierarchy"
                        xtype="pathfield"/>
                </items>
            </tab2>
        </items>
    </items>
</jcr:root>
