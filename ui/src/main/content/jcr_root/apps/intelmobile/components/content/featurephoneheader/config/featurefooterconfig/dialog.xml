<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Featurephone Footer Component"
    width="100%"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Footer Navigation Links">
                <items jcr:primaryType="cq:WidgetCollection">
                    <link1copy
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Link 1 Copy"
                        name="./link1copy"
                        xtype="textfield"/>
                    <link1url
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        editable="{Boolean}false"
                        fieldLabel="Link 1 URL"
                        name="./link1url"
                        predicate="hierarchy"
                        xtype="pathfield"/>
                    <link2copy
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Link 2 Copy"
                        name="./link2copy"
                        xtype="textfield"/>
                    <link2url
                        jcr:primaryType="cq:Widget"
                        editable="{Boolean}true"
                        fieldLabel="Link 2 URL"
                        name="./link2url"
                        predicate="hierarchy"
                        xtype="pathfield"/>
                    <link3copy
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Link 3 Copy"
                        name="./link3copy"
                        xtype="textfield"/>
                    <link3url
                        jcr:primaryType="cq:Widget"
                        editable="{Boolean}true"
                        fieldLabel="Link 3 URL"
                        name="./link3url"
                        predicate="hierarchy"
                        xtype="pathfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
