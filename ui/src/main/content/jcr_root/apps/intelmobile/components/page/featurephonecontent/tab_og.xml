<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Panel"
    title="Open Graph">
    <items jcr:primaryType="cq:WidgetCollection">
        <ogbasic
            jcr:primaryType="cq:Widget"
            collapsed="{Boolean}false"
            collapsible="{Boolean}false"
            xtype="dialogfieldset">
            <items jcr:primaryType="cq:WidgetCollection">
                <fbadmins
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Facebook Admins"
                    name="./fbadmins"
                    xtype="textfield"/>
                <fbappid
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Facebook App Id"
                    name="./fbappid"
                    xtype="textfield"/>
                <ogdescription
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Open Graph Description"
                    name="./ogdescription"
                    xtype="textarea"/>
                <ogimage
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Open Graph Image"
                    name="./ogimage"
                    xtype="pathfield"/>
                <ogsitename
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Open Graph Site Name"
                    name="./ogsitename"
                    xtype="textfield"/>
                <ogtitle
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Open Graph Title"
                    name="./ogtitle"
                    xtype="textfield"/>
                <ogtype
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Open Graph Type"
                    name="./ogtype"
                    xtype="textfield"/>
                <ogurl
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Open Graph URL"
                    name="./ogurl"
                    xtype="textfield"/>
            </items>
        </ogbasic>
    </items>
</jcr:root>
