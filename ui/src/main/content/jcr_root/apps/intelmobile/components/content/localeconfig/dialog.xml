<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="Locale Config Component"
    width="100%"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <general
                jcr:primaryType="cq:Panel"
                title="General">
                <items jcr:primaryType="cq:WidgetCollection">
                    <localeid
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        autoWidth="{Boolean}true"
                        fieldLabel="Locale ID"
                        name="./localeid"
                        regex="/^[a-zA-Z0-9-_]{0,12}$/"
                        regexText="You have exceeded maxlength"
                        xtype="textfield"/>
                    <currencycode
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        autoWidth="{Boolean}true"
                        fieldLabel="Currency Code"
                        name="./currencycode"
                        xtype="textfield"/>
                    <language
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        autoWidth="{Boolean}true"
                        fieldLabel="Language"
                        name="./language"
                        xtype="textfield"/>
                    <gsv
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        autoWidth="{Boolean}true"
                        fieldLabel="Google Site Verification"
                        name="./gsv"
                        xtype="textfield"/>
                </items>
            </general>
            <social
                jcr:primaryType="cq:Panel"
                title="Social Media Option">
                <items jcr:primaryType="cq:WidgetCollection">
                    <showfacebook
                        jcr:primaryType="nt:unstructured"
                        defaultValue="yes"
                        fieldLabel="Show Facebook"
                        name="./showfacebook"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <yes
                                jcr:primaryType="nt:unstructured"
                                text="yes"
                                value="yes"/>
                            <no
                                jcr:primaryType="nt:unstructured"
                                text="no"
                                value="no"/>
                        </options>
                    </showfacebook>
                    <showtweeter
