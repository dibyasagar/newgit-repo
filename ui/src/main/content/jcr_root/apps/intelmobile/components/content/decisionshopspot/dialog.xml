<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Decision Shop Spot"
    width="100%"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Decision Shop Spot">
                <items jcr:primaryType="cq:WidgetCollection">
                    <metaphoreslink
                        jcr:primaryType="cq:Widget"
                        editable="{Boolean}true"
                        fieldLabel="Metaphores Page URL"
                        name="./metaphoresUrl"
                        predicate="hierarchy"
                        xtype="pathfield"/>
                    <ultrabooklink
                        jcr:primaryType="cq:Widget"
                        editable="{Boolean}true"
                        fieldLabel="Ultrabook Page URL"
                        name="./ultrabookUrl"
                        predicate="hierarchy"
                        xtype="pathfield"/>
                    <suffleboardlink
                        jcr:primaryType="cq:Widget"
                        editable="{Boolean}true"
                        fieldLabel="Suffleboard Page URL"
                        name="./suffleboardUrl"
                        predicate="hierarchy"
                        xtype="pathfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
