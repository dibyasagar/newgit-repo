<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="400"
    stateful="false"
    width="100%"
    xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tabs jcr:primaryType="cq:TabPanel">
            <items jcr:primaryType="cq:WidgetCollection">
                <advanced
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_advanced.infinity.json"
                    xtype="cqinclude"/>
                <basic
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_basic.infinity.json"
                    xtype="cqinclude"/>
                <blueprint
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_blueprint.infinity.json"
                    xtype="cqinclude"/>
                <cloudservices
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_cloudservices.infinity.json"
                    xtype="cqinclude"/>
                <image
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_image.infinity.json"
                    xtype="cqinclude"/>
                <intelproperties
                    jcr:primaryType="cq:Panel"
                    title="Intel Properties">
                    <items jcr:primaryType="cq:WidgetCollection">
                        <articlecategory
                            jcr:primaryType="cq:Widget"
                            defaultValue="industryhottopics"
                            fieldLabel="Article Category"
                            name="./articleCategory"
                            type="select"
                            xtype="selection">
                            <options jcr:primaryType="cq:WidgetCollection">
                                <aboutintel
                                    jcr:primaryType="nt:unstructured"
                                    text="About Intel"
                                    value="aboutintel"/>
                                <industryhottopics
                                    jcr:primaryType="nt:unstructured"
                                    text="Industry Hot Topics"
                                    value="industryhottopics"/>
                                <intelprograms
                                    jcr:primaryType="nt:unstructured"
                                    text="Intel Programs"
                                    value="intelprograms"/>
                                <inteltechnologies
                                    jcr:primaryType="nt:unstructured"
                                    text="Intel Technologies"
                                    value="inteltechnologies"/>
                                <legalinformation
                                    jcr:primaryType="nt:unstructured"
                                    text="Legal Information"
                                    value="legalinformation"/>
                            </options>
                        </articlecategory>
                    </items>
                </intelproperties>
                <livecopy
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_livecopy.infinity.json"
                    xtype="cqinclude"/>
                <og
                    jcr:primaryType="cq:Widget"
                    path="/apps/intelmobile/components/page/mobilecontentpage/tab_og.infinity.json"
                    xtype="cqinclude"/>
                <mobile
                    jcr:primaryType="cq:Widget"
                    path="/libs/wcm/mobile/components/page/tab_mobile.infinity.json"
                    xtype="cqinclude"/>
            </items>
        </tabs>
    </items>
</jcr:root>
