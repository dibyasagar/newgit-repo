<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="Dialog"
    width="100%"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <general
                jcr:primaryType="cq:Panel"
                title="Jump To Menu">
                <items jcr:primaryType="cq:WidgetCollection">
                    <jumptolab
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        defaultValue="Jump to..."
                        fieldLabel="Label for Jump To"
                        name="./jumptolab"
                        xtype="textfield"/>
                    <jumptomenuitems
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./jumptomenuitems"
                        fieldDescription="Click the '+' to add a new menu item."
                        maxItems="12"
                        name="./jumptomenuitems"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <Copy_x0020_Text
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Copy Text"
                                name="./copytext"
                                xtype="textfield"/>
                            <internal_x0020_url
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                editable="{Boolean}true"
                                fieldLabel="Internal URL"
                                name="./internalUrl"
                                predicate="hierarchy"
                                xtype="pathfield"/>
                        </fieldConfig>
                    </jumptomenuitems>
                </items>
            </general>
            <templates_x0020_tab
                jcr:primaryType="cq:Panel"
                title="Last Visited Options">
                <items jcr:primaryType="cq:WidgetCollection">
                    <templateitems
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./templateitems"
                        fieldDescription="Click the '+' to add a new template item."
                        maxItems="12"
                        name="./templateitems"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <templatetype
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                cls="drpwidth"
                                defaultValue="Select"
                                fieldLabel="Select Template Type"
                                name="./templatetype"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <productcategory
                                        jcr:primaryType="nt:unstructured"
                                        text="Product Category"
                                        value="/apps/intelmobile/templates/productlisting"/>
                                    <productdetail
                                        jcr:primaryType="nt:unstructured"
                                        text="Product Detail"
                                        value="/apps/intelmobile/templates/productdetails"/>
                                    <productdetailcms
                                        jcr:primaryType="nt:unstructured"
                                        text="Product Detail CMS"
                                        value="/apps/intelmobile/templates/productdetailscms"/>
                                    <productshowcase
                                        jcr:primaryType="nt:unstructured"
                                        text="Product Showcase"
                                        value="/apps/intelmobile/templates/productshowcase"/>
                                    <productcompare
                                        jcr:primaryType="nt:unstructured"
                                        text="Product Compare"
                                        value="/apps/intelmobile/templates/productcompare"/>
                                    <decisiontoolresults
                                        jcr:primaryType="nt:unstructured"
                                        text="Decision Quiz Results"
                                        value="/apps/intelmobile/templates/decisionquizresults"/>
                                    <articlelanding
                                        jcr:primaryType="nt:unstructured"
                                        text="Article Landing"
                                        value="/apps/intelmobile/templates/articlelanding"/>
                                    <decisiontoolquestion
                                        jcr:primaryType="nt:unstructured"
                                        text="Decision Tool Question"
                                        value="/apps/intelmobile/templates/decisiontoolquestion"/>
                                    <sitesearchresults
                                        jcr:primaryType="nt:unstructured"
                                        text="Site Search Result"
                                        value="/apps/intelmobile/templates/mobilecontentpage"/>
                                </options>
                            </templatetype>
                            <displayname
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Display Copy"
                                name="./displayname"
                                xtype="textfield"/>
                            <showtemplate
                                jcr:primaryType="cq:Widget"
                                defaultValue="no"
                                fieldLabel="Show Jump To"
                                name="./showtemplatetype"
                                type="select"
                                width="100%"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <yes
                                        jcr:primaryType="nt:unstructured"
                                        text="yes"
                                        value="yes"/>
                                    <no
                                        jcr:primaryType="nt:unstructured"
                                        text="no"
                                        value="no"/>
                                </options>
                            </showtemplate>
                        </fieldConfig>
                    </templateitems>
                </items>
            </templates_x0020_tab>
        </items>
    </items>
</jcr:root>
